<div class="form-group">
  <%= f.label :branch_id, Review.human_attribute_name(:branch) %>
  <%= f.select :branch_id, support.branch_options, {include_blank: t(".select_branch")} %>
</div>
<%= render partial: "shared/reviews/criterion_input", collection: criterion_list,
  as: :criterion, locals: {f: f, options: rating_values} %>
<hr/>
<div class="form-group">
  <%= f.label :title, Review.human_attribute_name(:title) %>
  <%= f.text_field :title %>
</div>
<div class="form-group">
  <%= f.label :content, Review.human_attribute_name(:content) %>
  <%= f.text_area :content %>
</div>
