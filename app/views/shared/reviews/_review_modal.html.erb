<div class="modal fade review-modal" id="review_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content no-radius">
      <div class="modal-header white-bg">
        <div><%= t ".new_review" %></div>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div id="review_form">
        <%= render "shared/reviews/form", review: review, support: support %>
      </div>
    </div>
  </div>
</div>
