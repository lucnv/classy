<%= form_for q, url: management_verification_reviews_path, method: :get,
  html: {class: "auto-submit-form form-inline"} do |f| %>
  <%= f.select :status_eq, review_verification_status_options, {include_blank: "Chọn trạng thái"},
    class: "form-control mr-sm-2" %>
  <%= f.select :branch_slug_eq, support.branch_options, {include_blank: "Chọn chi nhánhh"},
    class: "form-control" %>
<% end %>
<br/>
