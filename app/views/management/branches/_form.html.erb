<div class="form-group">
  <%= f.label :name, Branch.human_attribute_name(:name) %>
  <%= f.text_field :name %>
</div>
<div class="form-group">
  <%= f.label :city_id, Branch.human_attribute_name(:city) %>
  <%= f.select :city_id, @support.city_options, {include_blank: t(".select_city")},
    class: "city_selection" %>
</div>
<div class="form-group">
  <%= f.label :district_id, Branch.human_attribute_name(:district) %>
  <%= f.select :district_id, @support.district_options, {include_blank: t(".select_district")},
    class: "district_selection" %>
</div>
<div class="form-group">
  <%= f.label :address, Branch.human_attribute_name(:address) %>
  <%= f.text_field :address %>
</div>
<div class="form-group">
  <%= f.label :phone_number, Branch.human_attribute_name(:phone_number) %>
  <%= f.text_field :phone_number %>
</div>
<div class="form-group">
  <%= f.label :email, Branch.human_attribute_name(:email) %>
  <%= f.email_field :email %>
</div>

<%= f.submit %>
