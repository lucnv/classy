<div class="filter-block border-bottom">
  <h3 class="block-title"><%= t ".location" %></h3>
  <div class="filter-list">
    <div class="form-group">
      <%= f.select :city_key_name_eq, search_form.city_options, {},
        class: "form-control city_filter" %>
    </div>
    <div class="form-group">
      <%= f.select :district_key_name_eq, search_form.district_options,
        {include_blank: t(".all_disticts")}, class: "form-control district_filter" %>
    </div>
  </div>
</div>
