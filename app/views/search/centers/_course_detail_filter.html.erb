<div class="filter-block">
  <h3 class="block-title"><%= t ".course_details" %></h3>
  <div class="filter-list">
    <div class="form-group">
      <%= f.select :center_category_key_name_eq, search_form.center_category_options,
        {}, class: "form-control center_category_filter" %>
    </div>
    <div class="form-group">
      <%= f.select :course_category_key_name_eq, search_form.course_category_options,
        {include_blank: t(".all_course_categories")},
        class: "form-control course_category_filter" %>
    </div>
    <div class="form-group">
      <%= f.select :course_sub_categories_key_name_eq, search_form.course_sub_category_options,
        {include_blank: t(".all_course_sub_categories")},
        class: "form-control course_sub_category_filter" %>
    </div>
  </div>
</div>
