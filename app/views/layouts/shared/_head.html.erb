<head>
  <title>Classy</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track": "reload" %>
  <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/10.0.0/css/bootstrap-slider.min.css" %>
  <%= javascript_include_tag "application", "data-turbolinks-track": "reload" %>
  <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/10.0.0/bootstrap-slider.min.js" %>
  <%= yield :include_tag_layout %>
  <%= javascript_include_tag "#{Settings.google_map.src}?key=#{Settings.google_map.key}
    &libraries=#{Settings.google_map.libraries}&sensor=#{Settings.google_map.sensor}
    &language=#{Settings.google_map.language}&region=#{Settings.google_map.region}" %>
</head>
